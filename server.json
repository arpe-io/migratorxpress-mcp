{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.arpe-io/migratorxpress-mcp",
  "description": "MCP server for MigratorXpress â€” automated cross-platform database migration with schema translation, parallel data transfer and built-in validation. Migrate from Oracle, SQL Server, PostgreSQL and Netezza to PostgreSQL or SQL Server with dual-level parallelism.",
  "repository": {
    "url": "https://github.com/arpe-io/migratorxpress-mcp",
    "source": "github"
  },
  "version": "0.1.1",
  "packages": [
    {
      "registryType": "pypi",
      "identifier": "migratorxpress-mcp",
      "version": "0.1.1",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "description": "Path to the MigratorXpress binary",
          "isRequired": true,
          "format": "string",
          "isSecret": false,
          "name": "MIGRATORXPRESS_PATH"
        },
        {
          "description": "Execution timeout in seconds (default: 1800)",
          "isRequired": false,
          "format": "string",
          "isSecret": false,
          "name": "MIGRATORXPRESS_TIMEOUT"
        }
      ]
    }
  ]
}
